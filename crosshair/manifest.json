{
  "manifest_version":"1",
  "type":"WebApp",
  "meta":{
    "name":"Ultimate Crosshair",
    "version":"1.0.0",
    "minimum-overwolf-version": "0.77.4.0",
    "author":"Tj Gienger",
    "icon":"cross1.png",
    "icon_gray":"cross2.png",
    "description":"Crosshair Overlay"
  },
  "data": {
    "start_window":"MainWindow",
    "windows":{
      "MainWindow":{
        "file":"files/index.html",
        "transparent":true,
        "resizable":true,
        "show_in_taskbar": true,
        "size": {
          "width": 1024,
          "height": 768
        },
        "start_position": {
          "Top": 0,
          "Left": 0
        }
      }
    },
    "plugins": [
      "npOverwolfSamplePlugin.dll"
    ]
  }
}
